import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner scn=new Scanner(System.in);
        int x=scn.nextInt();
        if(x<33)
        {
            x=x+4;
        }
        else
        {
            x=x;
        }
        System.out.println(x);
    }
}
